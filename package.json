    "babel-loader": "^9.1.3",
    "babelify": "^10.0.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-fbjs": "^4.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^8.0.3",
