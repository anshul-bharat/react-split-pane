    "eslint-plugin-relay": "^1.8.3",
    "mochify": "^9.2.0",
    "nyc": "^15.1.0",
    "parcel": "^2.12.0",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.23.1",
    "rimraf": "^5.0.7",
    "rollup": "^4.17.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
