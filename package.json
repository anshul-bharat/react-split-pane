  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "source": "src/index.js",
  "types": "index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "version": "0.1.93",
  "repository": {
    "type": "git",
    "url": "https://github.com/anshul-bharat/react-split-pane"
  },
  "license": "MIT",
  "bugs": {
